{
  "version": "0.2.0",
  "configurations": [
    {
    "name": "Python Debugger: Current File (venv)",
    "type": "debugpy",
    "request": "launch",
    "program": "${file}",
    "console": "integratedTerminal",
    "cwd": "${workspaceFolder}",
    "env": {
      "PYTHONPATH": "${workspaceFolder}"
    }
  }
]
}
